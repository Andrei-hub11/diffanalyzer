{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "console": "integratedTerminal",
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Launch File",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug Main",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "stopOnEntry": false,
      "args": [],
      "env": {},
      "showLog": true
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}"
    }
  ]
}

